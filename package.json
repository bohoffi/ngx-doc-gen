{
  "name": "ngx-doc-gen",
  "scripts": {
    "ng": "nx",
    "nx": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "demo:serve": "nx run ngx-doc-gen-demo:serve:development",
    "build": "nx run ngx-doc-gen:build",
    "lint": "nx run ngx-doc-gen:lint",
    "test": "nx run ngx-doc-gen:test",
    "docs": "npm run docs:generate && npm run docs:bundle && npm run docs:build",
    "docs:generate": "npm run docs:single:generate && npm run docs:multi:generate",
    "docs:single:generate": "npm run build && nx run single-entrypoint:doc-gen",
    "docs:multi:generate": "npm run build && nx run multi-entrypoint:doc-gen",
    "docs:bundle": "npm run docs:single:copy && npm run docs:multi:copy:one && npm run docs:multi:copy:two && npm run docs:multi:copy:three",
    "docs:single:copy": "cp ./dist/docs/single-test.html ./apps/ngx-doc-gen-demo/src/app/components/single-entrypoint/single-test.html",
    "docs:multi:copy:one": "cp ./dist/docs/multi-test-one.html ./apps/ngx-doc-gen-demo/src/app/components/multi-one/multi-test-one.html",
    "docs:multi:copy:two": "cp ./dist/docs/multi-test-two.html ./apps/ngx-doc-gen-demo/src/app/components/multi-two/multi-test-two.html",
    "docs:multi:copy:three": "cp ./dist/docs/multi-test-three.html ./apps/ngx-doc-gen-demo/src/app/components/multi-three/multi-test-three.html",
    "docs:build": "nx run ngx-doc-gen-demo:build:production --output-path docs --base-href /ngx-doc-gen/ --skip-nx-cache"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.12",
    "@angular/cdk": "13.3.9",
    "@angular/common": "13.3.12",
    "@angular/compiler": "13.3.12",
    "@angular/core": "13.3.12",
    "@angular/forms": "13.3.12",
    "@angular/material": "13.3.9",
    "@angular/platform-browser": "13.3.12",
    "@angular/platform-browser-dynamic": "13.3.12",
    "@angular/router": "13.3.12",
    "@nrwl/angular": "13.10.6",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1303.10",
    "@angular-devkit/build-angular": "13.3.10",
    "@angular-devkit/core": "13.3.10",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "13.3.10",
    "@angular/compiler-cli": "13.3.12",
    "@angular/language-service": "13.3.12",
    "@nrwl/cli": "13.10.6",
    "@nrwl/cypress": "13.10.6",
    "@nrwl/devkit": "13.10.6",
    "@nrwl/eslint-plugin-nx": "13.10.6",
    "@nrwl/jest": "13.10.6",
    "@nrwl/js": "13.10.6",
    "@nrwl/linter": "13.10.6",
    "@nrwl/workspace": "13.10.6",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "canonical-path": "^1.0.0",
    "cypress": "^9.1.0",
    "dgeni": "^0.4.14",
    "dgeni-packages": "^0.30.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.10.3",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "highlight.js": "^11.5.1",
    "jest": "27.2.3",
    "jest-preset-angular": "11.1.1",
    "jsonc-parser": "^3.0.0",
    "ng-packagr": "13.3.1",
    "postcss": "^8.4.5",
    "postcss-import": "^14.0.2",
    "postcss-preset-env": "^6.7.0",
    "postcss-url": "^10.1.1",
    "ts-jest": "27.0.5",
    "typescript": "4.6.4",
    "nx": "13.10.6"
  }
}
