{
  "name": "multi-entrypoint",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/multi-entrypoint/src",
  "prefix": "ngx-doc-gen",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/libs/multi-entrypoint"
      ],
      "options": {
        "project": "libs/multi-entrypoint/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/multi-entrypoint/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/multi-entrypoint/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/libs/multi-entrypoint"
      ],
      "options": {
        "jestConfig": "libs/multi-entrypoint/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "options": {
        "lintFilePatterns": [
          "libs/multi-entrypoint/src/**/*.ts",
          "libs/multi-entrypoint/src/**/*.html",
          "libs/multi-entrypoint/one/**/*.ts",
          "libs/multi-entrypoint/one/**/*.html",
          "libs/multi-entrypoint/two/**/*.ts",
          "libs/multi-entrypoint/two/**/*.html",
          "libs/multi-entrypoint/three/**/*.ts",
          "libs/multi-entrypoint/three/**/*.html"
        ]
      }
    },
    "doc-gen": {
      "executor": "./dist/libs/ngx-doc-gen:generate",
      "options": {
        "outputPath": "./dist/docs"
      }
    }
  },
  "tags": []
}
